<% for(var i = 0; i < users.length; i++) { %>
<tr>
    <th><%= i+1 %></th>
    <th><%= users[i].name %></th>
    <th><%= users[i].email %></th>
    <th><%= users[i].phone %></th>
    <th><%= users[i].contract %></th>
    <th>
        <a href="/update_user?id=<%= users[i]._id%>" class="text-success"><i class="fas fa-edit fa-lg mx-1" ></i></a>
        <a href="/delete/<%= users[i]._id%>" class="text-danger" data-id=<% users[i]._id %>><i class="fas fa-trash fa-lg mx-1" ></i></a>
        
    </th>
</tr>
<% } %>
